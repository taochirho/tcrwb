<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:top="@dimen/tile_depth"
        android:bottom="@dimen/tile_depth"
        android:left="@dimen/tile_depth"
        android:right="@dimen/tile_depth">

        <shape android:shape="rectangle">

            <solid android:color="?attr/tile_start_background" />

            <stroke
                android:width="1dp"
                android:color="?attr/tile_start_border" />
            <corners android:radius="@dimen/tile_inner_corner_radius"/>

        </shape>
    </item>

    <item android:drawable="@drawable/ic_baseline_timer_24"
        android:top="@dimen/tile_depth"
        android:bottom="@dimen/tile_depth"
        android:left="@dimen/tile_depth"
        android:right="@dimen/tile_depth"
        />
</layer-list>